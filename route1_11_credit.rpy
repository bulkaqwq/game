define end_credit_mode = 1

screen end_credit_return_menu:

    imagebutton:
        idle "gui/button/title-idle.png"
        hover "gui/button/title-hover.png"
        focus_mask "gui/button/title-hover.png"
        action [Play("click2", "music/Click To Start.ogg"), Jump("end_credit_leave")]
        xpos 50 ypos 620

label end_credit_leave:

    $ end_credit_mode = 1

    stop music fadeout 2.0

    scene black
    hide screen end_credit_return_menu
    $ renpy.transition(Dissolve(0.5))
    $ renpy.pause(3.0, hard='True')
    $ renpy.full_restart()

label end_credit_mode_menu:

    $ end_credit_mode = 2

    stop music fadeout 2.0

    $ renpy.music.set_volume(1.0, delay=0, channel='sound')
    play sound "music/Click To Start.ogg"

    hide screen about
    scene black
    $ renpy.transition(Dissolve(0.5))
    $ renpy.pause(3.0, hard='True')

    show screen end_credit_return_menu with Dissolve(0.2)

label end_credit:

    $ renpy.pause(4.0, hard=True)

    $ renpy.music.set_volume(0.8, delay=0, channel='sound')
    play sound "music/KILLSWITCH.ogg" fadeout 5.0

    show title_screen_1

    $ renpy.pause(2.0, hard='True')

    show title_screen_1_name
    $ renpy.transition(Dissolve(0.5))
    $ renpy.pause(1.0, hard='True')

    $ renpy.music.set_volume(1.0, delay=0, channel='music')
    play music "music/bgm/The End.ogg"

    $ renpy.pause(4.0, hard='True')

    show title_screen_1:
        ease 0.5 yoffset -450 alpha 0
    show title_screen_1_name:
        ease 0.5 yoffset -450 alpha 0

    show credit_1:
        yoffset 450 alpha 0
        ease 0.5 yoffset 0 alpha 1

    $ renpy.pause(4.0, hard='True')



    hide title_screen_1
